{
  "name": "commerce-algolia",
  "version": "5.0.35",
  "private": true,
  "engines": {
    "npm": ">=8.19.0",
    "node": ">=18.12.0",
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm"
  },
  "scripts": {
    "dev": "run-s uniform:pull:manifest dev:next",
    "dev:next": "next dev --port 3000",
    "build": "run-s uniform:pull:manifest build:next",
    "build:next": "next build",
    "start": "next start",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,scss}\"",
    "uniform:pull:manifest": "uniform context manifest download --output ./src/context/manifest.json",
    "uniform:pull:datatypes": "uniform canvas datatype pull ./content/datatypes",
    "uniform:pull:contenttype": "uniform canvas contenttype pull ./content/contenttype",
    "uniform:pull:entry": "uniform canvas entry pull ./content/entry",
    "uniform:pull:categories": "uniform canvas category pull ./content/categories",
    "uniform:pull:components": "uniform canvas component pull ./content/components",
    "uniform:pull:compositions": "uniform canvas composition pull ./content/compositions",
    "uniform:pull:project-map-definitions": "uniform project-map definition pull ./content/project-map-definitions",
    "uniform:pull:project-map-nodes": "uniform project-map node pull ./content/project-map-nodes",
    "uniform:pull:context:signals": "uniform context signal pull ./content/context/signals",
    "uniform:pull:context:enrichments": "uniform context enrichment pull ./content/context/enrichments",
    "uniform:pull:context:intents": "uniform context intent pull ./content/context/intents",
    "uniform:pull:context:audiences": "uniform context audience pull ./content/context/audiences",
    "uniform:pull:context:quirks": "uniform context quirk pull ./content/context/quirks",
    "uniform:pull:redirects": "uniform redirect definition pull ./content/redirects",
    "uniform:pull:context": "run-s uniform:pull:context:signals uniform:pull:context:enrichments uniform:pull:context:quirks uniform:pull:context:audiences uniform:pull:context:intents",
    "uniform:pull": "run-s uniform:pull:contenttype uniform:pull:entry uniform:pull:datatypes uniform:pull:categories uniform:pull:components uniform:pull:compositions uniform:pull:project-map-definitions uniform:pull:project-map-nodes uniform:pull:redirects uniform:pull:context",
    "uniform:push:datatypes": "uniform canvas datatype push ./content/datatypes",
    "uniform:push:contenttype": "uniform canvas contenttype push ./content/contenttype",
    "uniform:push:entry": "uniform canvas entry push ./content/entry",
    "uniform:push:categories": "uniform canvas category push ./content/categories",
    "uniform:push:components": "uniform canvas component push ./content/components",
    "uniform:push:compositions": "uniform canvas composition push ./content/compositions",
    "uniform:push:project-map-definitions": "uniform project-map definition push ./content/project-map-definitions",
    "uniform:push:project-map-nodes": "uniform project-map node push ./content/project-map-nodes",
    "uniform:push:context:signals": "uniform context signal push ./content/context/signals",
    "uniform:push:context:enrichments": "uniform context enrichment push ./content/context/enrichments",
    "uniform:push:context:intents": "uniform context intent push ./content/context/intents",
    "uniform:push:context:audiences": "uniform context audience push ./content/context/audiences",
    "uniform:push:context:quirks": "uniform context quirk push ./content/context/quirks",
    "uniform:push:redirects": "uniform redirect definition push ./content/redirects",
    "uniform:push:context": "run-s uniform:push:context:signals uniform:push:context:enrichments uniform:push:context:quirks uniform:push:context:audiences uniform:push:context:intents",
    "uniform:push": "run-s uniform:push:contenttype uniform:push:entry uniform:push:datatypes uniform:push:categories uniform:push:components uniform:push:compositions uniform:push:project-map-definitions uniform:push:project-map-nodes uniform:push:redirects uniform:push:context",
    "uniform:entry:publish": "uniform canvas entry push ./content/entry --state published",
    "uniform:compositions:publish": "uniform canvas composition push ./content/compositions --state published",
    "uniform:manifest:publish": "uniform context manifest publish",
    "uniform:publish": "run-s uniform:compositions:publish uniform:entry:publish uniform:manifest:publish",
    "import:products": "ts-node src/scripts/import-products",
    "uniform:get:redirects": "ts-node src/scripts/get-redirects.ts",
    "postinstall": "cd cli-run && npm install && npm run build",
    "cli": "npm i && cd cli-run && node dist/index.mjs"
  },
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^16.1.0",
    "@contentful/rich-text-types": "^16.2.0",
    "@segment/snippet": "4.16.0",
    "@uniformdev/canvas": "19.66.1",
    "@uniformdev/canvas-next": "19.66.1",
    "@uniformdev/canvas-react": "19.66.1",
    "@uniformdev/context": "19.66.1",
    "@uniformdev/context-devtools": "19.66.1",
    "@uniformdev/context-gtag": "19.66.1",
    "@uniformdev/context-next": "19.66.1",
    "@uniformdev/context-react": "19.66.1",
    "@uniformdev/project-map": "19.66.1",
    "@uniformdev/redirect": "19.66.1",
    "@uniformdev/richtext": "19.66.1",
    "@vercel/og": "^0.5.7",
    "classnames": "^2.3.2",
    "cookie": "^0.5.0",
    "daisyui": "^2.51.5",
    "framer-motion": "10.16.2",
    "lodash.debounce": "^4.0.8",
    "next": "13.4.19",
    "nextjs-cors": "^2.1.2",
    "npm-run-all": "4.1.5",
    "react": "18.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "18.2.0",
    "react-instantsearch-hooks-web": "6.45.0",
    "react-multi-carousel": "^2.8.3",
    "react-player": "^2.12.0",
    "react-responsive-masonry": "2.1.7",
    "sass": "1.64.1",
    "sharp": "^0.32.1"
  },
  "devDependencies": {
    "@algolia/recommend": "4.18.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/color": "^3.0.3",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.4",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@types/react-responsive-masonry": "2.1.0",
    "@types/segment-analytics": "0.0.34",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@typescript-eslint/types": "^6.4.0",
    "@uniformdev/cli": "19.66.1",
    "algoliasearch": "4.18.0",
    "autoprefixer": "^10.4.14",
    "contentful": "^9.3.5",
    "eslint": "^8.47.0",
    "eslint-config-custom": "*",
    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.2",
    "shared": "*",
    "tailwind-config": "*",
    "tailwindcss": "^3.3.1",
    "ts-node": "^10.9.1",
    "typescript": "5.2.2"
  }
}
